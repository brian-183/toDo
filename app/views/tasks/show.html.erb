

<p><em><%= @task.details%></em></p>
<p>Completed? 
<%= link_to(
        check_box_tag(:task, :completed, @task.completed),
       "/tasks/#{@task.id}/complete/#{!@task.completed}")%></p>
<%=render 'form'%><br>
<%= link_to 'Edit Task', edit_task_path(@task) %>
<%= link_to 'Delete', task_path(@task), method: :delete %>
<%= link_to 'Tasks List', tasks_index_path %>